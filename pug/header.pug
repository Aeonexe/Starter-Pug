doctype html

html
	mixin metatags(title, description)
		head
			<!-- info -->
			meta(charset="UTF-8")
			meta(http-equiv="content-type" content="text/html; charset=UTF-8")
			meta(http-equiv="language" content="es-MX")
			<!-- Site info -->
			title= title
			meta(name="description" content=description)
